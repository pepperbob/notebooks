    "import bcrypt\n",
    "\n",
    "password = input(\"Password\")\n",
    "salt = bcrypt.gensalt()\n",
    "\n",
    "hashed = bcrypt.hashpw(password.encode(\"utf-8\"), salt).decode(\"utf-8\")\n",
    "\n",
    "print(f\"Hash for {password}:\")\n",
    "print(hashed)"
   ]
